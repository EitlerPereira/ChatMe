<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.junyu.CorpBudge">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name="com.junyu.CorpBudge.MoneyManagerApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.junyu.CorpBudge.MainActivity"
            android:screenOrientation="portrait">

        </activity>

        <activity
            android:name="com.junyu.CorpBudge.MainPageActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.junyu.CorpBudge.GroupPageActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.junyu.CorpBudge.SignUpActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.junyu.CorpBudge.TransactionActivity"
            android:screenOrientation="portrait"></activity>

        <activity
            android:name="com.junyu.CorpBudge.activity.NewMainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

    </application>

</manifest>